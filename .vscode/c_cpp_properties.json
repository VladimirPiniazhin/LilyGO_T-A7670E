{
    "configurations": [
        {
            "name": "ESP-IDF",
            "includePath": [
                
                "${workspaceFolder}/**",
                "${workspaceFolder}/main",
                "${workspaceFolder}/components/**",
                
               
                "${env:IDF_PATH}/components/**",
                "${env:IDF_PATH}/components/bt/host/nimble/nimble/porting/nimble/include",
                "${env:IDF_PATH}/components/esp-protocols/components/esp_modem/include",
                "${env:IDF_PATH}/components/esp-protocols/components/esp_modem/include/cxx_include",
                "${env:IDF_PATH}/components/esp_http_client/include",
                "${env:IDF_PATH}/components/esp_netif/lwip/netif",
                "${env:IDF_PATH}/components/esp_netif/lwip/include",
                "${env:IDF_PATH}/components/esp_netif/lwip/include/lwip",
                "${env:IDF_PATH}/components/esp_netif/**",
                
                
                "${env:IDF_TOOLS_PATH}/tools/xtensa-esp-elf/esp-14.2.0_20241119/xtensa-esp-elf/lib/gcc/xtensa-esp-elf/14.2.0/include",
                "${env:IDF_TOOLS_PATH}/tools/xtensa-esp-elf/esp-14.2.0_20241119/xtensa-esp-elf/xtensa-esp-elf/include"

            ],
            "defines": [
                "ESP_PLATFORM",
                "IDF_VER=\"v5.4\"",
                "XTENSA",
                "_GNU_SOURCE",
                "configTICK_RATE_HZ=1000"
            ],
            "compilerPath": "${env:IDF_TOOLS_PATH}/tools/xtensa-esp-elf/esp-14.2.0_20241119/xtensa-esp-elf/bin/xtensa-esp32-elf-gcc.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compileCommands": [
                "${workspaceFolder}/build/compile_commands.json"
            ]
        }
    ],
    "version": 4
}